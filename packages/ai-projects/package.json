{
  "name": "ai-projects",
  "version": "0.1.0",
  "description": "AI-driven project management with human approval workflows",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "project-management",
    "workflow",
    "approval",
    "human-in-the-loop",
    "okr",
    "objectives",
    "key-results",
    "plans",
    "agents",
    "delegation"
  ],
  "author": "Drivly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai"
  },
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "homepage": "https://mdx.org.ai",
  "dependencies": {
    "ai-management": "workspace:*",
    "autonomous-agents": "workspace:*",
    "business-as-code": "workspace:*",
    "ai-business": "workspace:*"
  },
  "peerDependencies": {
    "ai-management": "*",
    "autonomous-agents": "*",
    "business-as-code": "*",
    "ai-business": "*"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^1.2.1"
  }
}
