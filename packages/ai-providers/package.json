{
  "name": "ai-providers",
  "version": "0.2.0",
  "description": "Provider router for AI models including OpenAI, Anthropic, Google, and more",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit",
    "prepublishOnly": "pnpm clean && pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^1.0.0",
    "@ai-sdk/anthropic": "^1.2.0",
    "@ai-sdk/assemblyai": "^1.0.0",
    "@ai-sdk/azure": "^1.0.0",
    "@ai-sdk/cerebras": "^1.0.0",
    "@ai-sdk/cohere": "^1.0.0",
    "@ai-sdk/deepgram": "^1.0.0",
    "@ai-sdk/deepinfra": "^1.0.0",
    "@ai-sdk/deepseek": "^1.0.0",
    "@ai-sdk/elevenlabs": "^0.0.2",
    "@ai-sdk/fal": "^1.0.0",
    "@ai-sdk/fireworks": "^1.0.0",
    "@ai-sdk/gladia": "^1.0.0",
    "@ai-sdk/google": "^1.2.1",
    "@ai-sdk/google-vertex": "^2.2.18",
    "@ai-sdk/groq": "^1.0.0",
    "@ai-sdk/hume": "^1.0.0",
    "@ai-sdk/lmnt": "^1.0.0",
    "@ai-sdk/luma": "^1.0.0",
    "@ai-sdk/mistral": "^1.0.0",
    "@ai-sdk/openai": "^1.3.16",
    "@ai-sdk/perplexity": "^1.0.0",
    "@ai-sdk/replicate": "^1.0.0",
    "@ai-sdk/revai": "^1.0.0",
    "@ai-sdk/together": "^1.0.0",
    "@ai-sdk/xai": "^1.0.0",
    "ai": "^4.3.9"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
