{
  "name": "ai-providers",
  "version": "0.2.0",
  "description": "Provider router for AI models including OpenAI, Anthropic, Google, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit",
    "prepublishOnly": "pnpm clean && pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^1.0.0",
    "@ai-sdk/anthropic": "^1.2.0",
    "@ai-sdk/assemblyai": "^0.0.1",
    "@ai-sdk/azure": "^1.3.22",
    "@ai-sdk/cerebras": "^0.2.13",
    "@ai-sdk/cohere": "^1.2.9",
    "@ai-sdk/deepgram": "^0.0.1",
    "@ai-sdk/deepinfra": "^0.2.14",
    "@ai-sdk/deepseek": "^0.2.13",
    "@ai-sdk/elevenlabs": "^0.0.2",
    "@ai-sdk/fal": "^0.1.10",
    "@ai-sdk/fireworks": "^0.2.13",
    "@ai-sdk/gladia": "^0.0.1",
    "@ai-sdk/google": "^1.2.1",
    "@ai-sdk/google-vertex": "^2.2.18",
    "@ai-sdk/groq": "^1.0.0",
    "@ai-sdk/hume": "^0.0.1",
    "@ai-sdk/lmnt": "^0.0.1",
    "@ai-sdk/luma": "^0.1.7",
    "@ai-sdk/mistral": "^1.2.7",
    "@ai-sdk/openai": "^1.3.16",
    "@ai-sdk/perplexity": "^1.0.0",
    "@ai-sdk/replicate": "^0.2.7",
    "@ai-sdk/revai": "^0.0.1",
    "@ai-sdk/xai": "^1.0.0",
    "ai": "^4.3.9"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "esbuild": "^0.25.2",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
