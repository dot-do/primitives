{
  "name": "ai-startups",
  "version": "0.1.0",
  "description": "Startup types and functions for AI applications combining Business and Service patterns",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ai-business": "workspace:*",
    "ai-service": "workspace:*",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "esbuild": "^0.25.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.2",
    "tsup": "^8.4.0",
    "vitest": "^1.4.0",
    "semantic-release": "^23.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git"
  },
  "homepage": "https://mdx.org.ai",
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "keywords": [
    "ai",
    "business",
    "startup",
    "services",
    "site-generator",
    "database"
  ],
  "license": "MIT"
}
