{
  "name": "ai-manager",
  "version": "0.1.0",
  "description": "A function-based Manager API for defining Objectives and Key Results, creating Plans, and delegating tasks to Agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "manager",
    "okr",
    "objectives",
    "key-results",
    "plans",
    "agents",
    "delegation"
  ],
  "author": "Drivly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai"
  },
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "homepage": "https://mdx.org.ai",
  "dependencies": {
    "autonomous-agents": "workspace:*",
    "ai-business": "workspace:*"
  },
  "peerDependencies": {
    "autonomous-agents": "*",
    "ai-business": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
