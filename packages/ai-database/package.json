{
  "name": "ai-database",
  "version": "0.1.0",
  "description": "Minimalistic package that exports Payload collections and Next.js API route handlers",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai-database": "./dist/bin.js",
    "ai-database-admin": "./dist/bin.js"
  },
  "files": [
    "dist",
    "admin/.next/standalone",
    "admin/.next/static",
    "admin/public"
  ],
  "scripts": {
    "build": "tsc && cd admin && npm install && npm run build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@payloadcms/db-sqlite": "3.33.0",
    "@payloadcms/next": "3.33.0",
    "@payloadcms/payload-cloud": "3.33.0",
    "@payloadcms/plugin-multi-tenant": "^3.37.0",
    "@payloadcms/richtext-lexical": "3.33.0",
    "@vercel/functions": "^2.0.3",
    "ai": "^4.3.15",
    "camelcase-keys": "^8.0.0",
    "cross-env": "^7.0.3",
    "dedent": "^1.6.0",
    "graphql": "^16.8.1",
    "gray-matter": "^4.0.3",
    "next": "15.3.0",
    "payload": "3.33.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "zod": "^3.24.4"
  },
  "peerDependencies": {
    "@payloadcms/db-mongodb": "^3.0.0",
    "@payloadcms/db-postgres": "^3.0.0",
    "@payloadcms/db-sqlite": "^3.0.0",
    "next": "^14.0.0 || ^15.0.0",
    "payload": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@payloadcms/db-mongodb": {
      "optional": true
    },
    "@payloadcms/db-postgres": {
      "optional": true
    },
    "@payloadcms/db-sqlite": {
      "optional": false
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "resolutions": {
    "date-fns": "2.30.0"
  }
}
