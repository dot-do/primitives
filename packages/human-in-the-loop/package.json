{
  "name": "human-in-the-loop",
  "version": "0.1.0",
  "description": "Strongly-typed interface for human functions across multiple platforms",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "human",
    "in-the-loop",
    "feedback",
    "slack",
    "teams",
    "react",
    "email"
  ],
  "author": "Drivly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git"
  },
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "homepage": "https://primitives.org.ai",
  "dependencies": {
    "zod": "^3.24.3",
    "ai-providers": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-email": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  }
}
