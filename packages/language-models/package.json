    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "node-fetch": "^3.3.0",
    "prettier": "^3.0.0",
    "release-it": "^19.0.2",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
