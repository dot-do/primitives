{
  "name": "@ai-primitives/language-models",
  "version": "0.1.0",
  "description": "Language model integrations for AI primitives",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "fetch-models": "tsx scripts/fetch-models.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write .",
    "release": "release-it"
  },
  "keywords": [
    "ai",
    "language-models",
    "openrouter",
    "ai-primitives"
  ],
  "author": "AI Primitives",
  "license": "MIT",
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "release-it": "^17.11.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
