{
  "name": "ai-examples",
  "version": "0.1.0",
  "description": "Examples for AI product/service idea generation and business development",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ai",
    "examples",
    "business",
    "ideas"
  ],
  "author": "Drivly",
  "license": "MIT",
  "dependencies": {
    "ai-database": "workspace:*",
    "ai-functions": "workspace:*",
    "ai-business": "workspace:*",
    "ai-waitlist": "workspace:*",
    "ai-workflows": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.0.4"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "format": ["cjs", "esm"],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
