{
  "name": "ai-plans",
  "version": "0.1.0",
  "description": "Create and manage AI-driven plans with goals and key results",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "ai-functions": "workspace:*",
    "ai-providers": "workspace:*",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "dotenv": "^16.4.5",
    "esbuild": "^0.25.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.2",
    "tsup": "^8.4.0",
    "vitest": "^3.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git"
  },
  "homepage": "https://mdx.org.ai",
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "keywords": [
    "ai",
    "plans",
    "goals",
    "okr",
    "key-results"
  ],
  "license": "MIT"
}
