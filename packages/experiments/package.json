{
  "name": "@ai-primitives/experiments",
  "version": "0.1.0",
  "description": "A minimalistic experiment runner for AI tasks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ai",
    "experiments",
    "testing",
    "vitest"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'"
  },
  "devDependencies": {
    "@types/node": "^22.15.6",
    "prettier": "^3.4.2",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vitest": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git"
  },
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "homepage": "https://mdx.org.ai",
  "license": "MIT"
}
