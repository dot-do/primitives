{
  "name": "services-as-software",
  "version": "0.1.0",
  "description": "Higher-order abstraction layer for composing services into a cohesive business entity",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "services-as-software": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "services",
    "business",
    "startup",
    "composition",
    "pricing",
    "events"
  ],
  "author": "Drivly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git"
  },
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "homepage": "https://mdx.org.ai",
  "dependencies": {
    "ai-service": "workspace:*",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "esbuild": "^0.25.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.2",
    "tsup": "^8.0.2",
    "vitest": "^3.0.9"
  },
  "engines": {
    "node": ">=18"
  }
}
