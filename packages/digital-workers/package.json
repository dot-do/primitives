{
  "name": "digital-workers",
  "version": "0.1.0",
  "description": "Enhanced autonomous digital workers with event loop, KPI tracking, and communication capabilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.ts\" --semi false --single-quote true --jsx-single-quote true --print-width 160 --tab-width 2"
  },
  "keywords": [
    "agent",
    "autonomous",
    "ai",
    "digital-worker",
    "kpi",
    "okr"
  ],
  "author": "Drivly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git",
    "directory": "packages/digital-workers"
  },
  "homepage": "https://mdx.org.ai",
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "dependencies": {
    "autonomous-agents": "workspace:*",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
