{
  "name": "ai-components",
  "version": "0.1.0",
  "description": "Pre-wrapped UI components that leverage AI generation capabilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ai",
    "react",
    "components",
    "ui",
    "shadcn"
  ],
  "author": "Drivly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git"
  },
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "homepage": "https://mdx.org.ai",
  "dependencies": {
    "ai-props": "^0.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "next-mdx-remote": "^5.0.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.20",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "lucide-react": "^0.358.0",
    "next-mdx-remote": "^5.0.0",
    "prettier": "^3.4.2",
    "tailwind-merge": "^2.6.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  }
}
